<?xml version="0.0" ?>
<robot name="model.urdf">
  <link name="baseLink">
    <contact>
    	<friction_anchor/>
      <lateral_friction value="1.0"/>
    </contact>
    <inertial>
      <origin rpy="0 3.1415926 0" xyz="0 0 0"/>
       <mass value="0.491"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
        <!-- for digit -->
        <mesh filename="textured.obj" scale="1.2 1.2 1.2"/>
        <!-- for digitac and tactip -->
        <!-- <mesh filename="textured.obj" scale="1.15 1.15 1.15"/> -->
      </geometry>
      <material name="white">
        <color rgba="1. 1. 1. 1."/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 3.1415926 0" xyz="0 0 0"/>
      <geometry>
				<box size="0.08 0.08 .1" />
    	 	<!-- <mesh filename="collision.obj" scale="1 1 1"/> -->
      </geometry>
    </collision>
  </link>
</robot>